{
  "version": 2,
  "builds": [
    { "src": "api/save.py", "use": "@vercel/python" },
    { "src": "api/search.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/save", "dest": "/api/save.py" },
    { "src": "/api/search", "dest": "/api/search.py" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
